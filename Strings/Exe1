#include <iostream>
#include <string>

using namespace std;

int main(){

    string frase1 = "bola";
    string frase2 = "bolas";

    if( frase1 == frase2 ){
        cout << "eh igual" << endl;
    }else{
        cout << "não eh igual" << endl;
    }

    //OU
    cout << endl << "## outro metodo usando compare.##" << endl;
    if(!frase1.compare(frase2)){
        cout << "eh igual" << endl;
    }else{
        cout << "não eh igual" << endl;
    }
    
    return 0;

}
